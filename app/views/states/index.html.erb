<%@states.each do |state| %>
    <div class="col-lg-10 col-xs-offset-1 state-content">
    <div class="panel panel-default">
      <div class="panel-heading" data-toggle="collapse" href="#<%=state.name%>">
        <h2 class="category-title panel-title">
          <%=state.name%>
        </h2>
      </div>
      <div id="<%=state.name%>" class="category-content panel-collapse collapse in">
        <div class="panel-body">
          <div class="row">
            <%state.cities.each do |city| %>
            <div class="col-lg-3">
              <%= link_to "#{city.name}", city_path(city.name) %>
            </div>
            <% end %>
          </div>
            </div>
          </div>

        </div>
    </div>
    <% end%>
  </div>
